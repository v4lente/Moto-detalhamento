  "scripts": {
    "dev:client": "vite dev --port 5000",
    "dev": "cross-env NODE_ENV=development tsx --env-file=.env server/index.ts",
    "fix-esbuild": "node script/fix-esbuild-permissions.mjs",
    "prebuild": "node script/fix-esbuild-permissions.mjs",
    "build": "node script/build.mjs",
    "start": "NODE_ENV=production node dist/index.cjs",
    "check": "tsc",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "db:push": "tsx --env-file=.env node_modules/.bin/drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx --env-file=.env db/migrate.ts",
    "db:seed": "tsx --env-file=.env seed.ts",
    "db:seed:fresh": "tsx --env-file=.env seed.ts --fresh",
    "db:setup": "npm run db:migrate && npm run db:seed:fresh"
